{
  "name": "mf-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:root",
    "startr": "ng serve root --port 4000",
    "startm": "ng serve mfe1 --port 3000",
    "start:all": "concurrently \"npm run start:root\" \"npm run start:mfe1\" ",
    "build:root": "ng build root --prod",
    "build:mfe1": "ng build mfe1 --prod",
    "serve:dist": "concurrently \"serve dist/root -l 4000 -s\" \"serve dist/mfe1 -l 3000 -s\" ",
    "build": "npm run build:root && npm run build:mfe1",
    "test": "ng test",
    "lint": "ng lint",
